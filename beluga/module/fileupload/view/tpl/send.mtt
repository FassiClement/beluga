<link rel="stylesheet" href="::base_url::/beluga/fileupload/css/send"/>
<div class="container fileupload-upload-widget">
    <div class="container-fluid">
        <h4>Select a new file to upload !</h4>
        <form action="::base_url::/beluga/fileupload/send" method="POST" enctype="multipart/form-data"><br>
            <input type="file" name="fileupload_filename" id="fileupload_filename" class="btn btn-default">
            <input type="submit" name="submit" class="btn btn-warning pull-right fileupload-upload-submit">
        </form>
    </div>
</div>